config_version=5

[application]

config/name="Openscop Rewritten"
run/main_scene="res://scene/management/boot.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Openscop"
config/features=PackedStringArray("4.1", "Forward Plus")
run/max_fps=30
config/icon="res://gift.png"

[autoload]

HUD="*res://scene/management/hud.tscn"
Global="*res://scene/management/global.tscn"
EventBus="*res://scene/management/event_bus.tscn"
BGMusic="*res://scene/management/bgmusic.tscn"
SaveManager="*res://scene/management/save_manager.tscn"
Console="*res://scene/debug/console.tscn"
RecordingManager="*res://scene/management/recording_manager.tscn"
GameManager="*res://scene/management/game_manager.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=240
window/size/window_width_override=640
window/size/window_height_override=480
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

